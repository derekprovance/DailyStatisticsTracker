<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="113.0" prefWidth="473.0" style="-fx-background-color: #f6f6f5;">
         <children>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#f2f0f0" height="51.0" layoutX="17.0" layoutY="61.0" stroke="BLACK" strokeType="INSIDE" width="153.0" />
            <Button fx:id="submitButton" layoutX="345.0" layoutY="86.0" mnemonicParsing="false" onMouseClicked="#submitForm" text="Submit" />
            <Slider fx:id="dayRating" layoutX="23.0" layoutY="90.0" />
            <ToggleButton fx:id="successDay" layoutX="130.0" layoutY="18.0" mnemonicParsing="false" text="Success" />
            <Label layoutX="17.0" layoutY="23.0" text="Activities Today?" />
            <Label layoutX="25.0" layoutY="65.0" text="Day Rating" />
            <Button fx:id="closeButton" layoutX="415.0" layoutY="86.0" mnemonicParsing="false" onMouseClicked="#closeWindow" text="Close" />
            <Label fx:id="errorMessage" layoutX="323.0" layoutY="14.0" prefHeight="27.0" prefWidth="159.0" text="Operation Failure" textFill="RED" visible="false">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <TextArea fx:id="note" layoutX="212.0" layoutY="10.0" prefHeight="70.0" prefWidth="256.0" promptText="Anything special about today..?" wrapText="true" />
         </children>
      </Pane>
   </children>
</GridPane>
